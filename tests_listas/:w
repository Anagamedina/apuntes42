# include <unistd.h>
# include <stdlib.h>
# include <stdio.h>

typedef struct s_list
{
	void			*content;
	struct s_list	*next;
}		t_list;

int	main()
{
	t_list	*lista = (t_list *) malloc(sizeof(t_list));

	t_list	*node1 = (t_list *) malloc(sizeof(t_list));
	t_list	*node2 = (t_list *) malloc(sizeof(t_list));
	t_list	*node3 = (t_list *) malloc(sizeof(t_list));

	node1->content = "First";
	node1->next = NULL;
	node2->content = "second";
	node2->next = NULL;
	node3->content = "third";
	node3->next = NULL;

	node1->next = node2;
	node2->next = node3;
    node3->next = node4;
    node4->next = node5;



	// AGREGAR 
    t_list	*node4 = (t_list *) malloc(sizeof(t_list));
    node4->content = "fourth";
    node4->next = NULL;


	return (0);
}
